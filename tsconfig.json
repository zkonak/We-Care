{
  "compilerOptions": {
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "strictPropertyInitialization": false,
      "target": "es6",
      "module": "CommonJs",
      "lib": [
          "es5",
          "es6"
      ],
      "allowJs": true, //false?
      "outDir": "build",
      "rootDir": "src",
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "resolveJsonModule": true
  }
}